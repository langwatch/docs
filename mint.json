{
  "$schema": "https://mintlify.com/schema.json",
  "name": "LangWatch",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#ed8926",
    "light": "#f2a65a",
    "dark": "#ed8926",
    "anchors": {
      "from": "#ed8926",
      "to": "#ed8926"
    }
  },
  "topbarLinks": [
    {
      "name": "llms.txt",
      "url": "https://docs.langwatch.ai/llms.txt"
    },
    {
      "name": "Support",
      "url": "/support#email-support"
    },
    {
      "name": "Dashboard",
      "url": "https://app.langwatch.ai"
    }
  ],
  "topbarCtaButton": {
    "type": "github",
    "url": "https://github.com/langwatch/langwatch"
  },
  "anchors": [
    {
      "name": "Open Dashboard",
      "icon": "link",
      "url": "https://app.langwatch.ai"
    },
    {
      "name": "GitHub Repo",
      "icon": "github",
      "url": "https://github.com/langwatch/langwatch"
    }
  ],
  "navigation": [
    {
      "group": "",
      "pages": ["introduction", "self-hosting"]
    },
    {
      "group": "LLM Observability",
      "pages": [
        "integration/overview",
        "concepts",
        {
          "group": "Language APIs & SDKs",
          "pages": [
            {
              "group": "Python",
              "pages": [
                "integration/python/guide",
                "integration/python/reference"
              ]
            },
            {
              "group": "TypeScript",
              "pages": ["integration/typescript/guide"]
            },
            {
              "group": "OpenTelemetry",
              "pages": ["integration/opentelemetry/guide"]
            },
            "integration/rags-context-tracking",
            "integration/langflow",
            "integration/flowise",
            "integration/rest-api"
          ]
        },
        {
          "group": "User Events",
          "pages": [
            "user-events/overview",
            {
              "group": "Events",
              "pages": [
                "user-events/thumbs-up-down",
                "user-events/waited-to-finish",
                "user-events/selected-text",
                "user-events/custom"
              ]
            }
          ]
        },
        {
          "group": "Monitoring & Alerts",
          "pages": ["features/triggers", "features/embedded-analytics"]
        },
        "integration/code-examples"
      ]
    },
    {
      "group": "LLM Evaluation",
      "pages": [
        {
          "group": "Evaluation",
          "pages": [
            "llm-evaluation/offline-evaluation",
            "llm-evaluation/real-time-evaluation",
            "evaluations/overview",
            "evaluations/custom-evaluator-integration"
          ]
        },
        "features/datasets",
        "features/annotations"
      ]
    },
    {
      "group": "LLM Development",
      "pages": [
        {
          "group": "Prompt Optimization Studio",
          "pages": [
            "optimization-studio/overview",
            "optimization-studio/llm-nodes",
            "optimization-studio/datasets",
            "optimization-studio/evaluating",
            "optimization-studio/optimizing"
          ]
        },
        {
          "group": "DSPy Visualization",
          "pages": [
            "dspy-visualization/quickstart",
            "dspy-visualization/custom-optimizer",
            "dspy-visualization/rag-visualization"
          ]
        },
        "integration/mcp"
      ]
    },
    {
      "group": "API Endpoints",
      "pages": [
        {
          "group": "Traces",
          "pages": [
            "api-reference/traces/overview",
            "api-reference/traces/get-trace-details",
            "api-reference/traces/search-traces",
            "api-reference/traces/create-public-trace-path",
            "api-reference/traces/delete-public-trace-path"
          ]
        },
        {
          "group": "Annotations",
          "pages": [
            "api-reference/annotations/overview",
            "api-reference/annotations/get-annotation",
            "api-reference/annotations/get-single-annotation",
            "api-reference/annotations/delete-annotation",
            "api-reference/annotations/patch-annotation",
            "api-reference/annotations/get-all-annotations-trace",
            "api-reference/annotations/create-annotation-trace"
          ]
        },
        {
          "group": "Datasets",
          "pages": ["api-reference/datasets/post-dataset-entries"]
        }
      ]
    },
    {
      "group": "Support",
      "pages": ["support", "status"]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/langwatch/langwatch",
    "website": "https://langwatch.ai",
    "x-twitter": "https://x.com/langwatchai",
    "linkedin": "https://www.linkedin.com/company/langwatch/",
    "youtube": "https://www.youtube.com/@LangWatch"
  },
  "redirects": [
    {
      "source": "/langevals/:path*",
      "destination": "https://github.com/langwatch/langevals"
    },
    {
      "source": "/integration/cookbooks",
      "destination": "/integration/code-examples"
    },
    {
      "source": "/guardrails/:path*",
      "destination": "/llm-evaluation/real-time-evaluation"
    }
  ],
  "modeToggle": {
    "default": "light"
  }
}
